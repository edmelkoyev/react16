{   
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "env": {
        "browser": true,
        "jest":true
    },
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "rules": {
        "quotes": ["error", "single"],
        "strict": ["error", "never"],
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/react-in-jsx-scope": 2
      },
      "plugins": [
        "react"
      ]
 }